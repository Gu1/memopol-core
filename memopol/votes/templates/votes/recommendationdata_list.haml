-extends "base.html"

-block content

  %h1
    -trans "Importable votes"
  -regroup object_list by proposal_name as object_list

  %ul.votes
    -for rd in object_list reversed
      %li
        =rd.grouper
        %ul
          -for r in rd.list
            %li
              %a{href: "{% url votes:import_vote r.id %}"}
                {{ r.date|date:"d/m/Y" }} - {{ r.title }}
      %br
